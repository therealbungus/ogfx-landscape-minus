	tmpl_slope_to_ne(corner_se)
	tmpl_slope_to_ne(corner_sw)
	tmpl_slope_to_ne(corner_nw)
	tmpl_slope_to_ne(small_ne)
	tmpl_slope_to_ne(small_se)
	tmpl_slope_to_ne(small_sw)
	tmpl_slope_to_ne(small_nw)

	tmpl_slope_to_sw(bank_ne)
