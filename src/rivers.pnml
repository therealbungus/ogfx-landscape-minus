#define var_river_set 1

template tmpl_riversprites_normal(
		bank_ne, bank_se, bank_sw, bank_nw,
		corner_ne, corner_se, corner_sw, corner_nw,
		small_ne, small_se, small_sw, small_nw
	 ) {
	tmpl_flat_water(bank_ne)
	tmpl_flat_water(bank_se)
	tmpl_flat_water(bank_sw)
	tmpl_flat_water(bank_nw)
	tmpl_flat_water(corner_ne)
	tmpl_flat_water(corner_se)
	tmpl_flat_water(corner_sw)
	tmpl_flat_water(corner_nw)
	tmpl_flat_water(small_ne)
	tmpl_flat_water(small_se)
	tmpl_flat_water(small_sw)
	tmpl_flat_water(small_nw)

	tmpl_slope_to_se(bank_ne)
	tmpl_slope_to_se(bank_se)
	tmpl_slope_to_se(bank_sw)
	tmpl_slope_to_se(bank_nw)
	tmpl_slope_to_se(corner_ne)
	tmpl_slope_to_se(corner_se)
	tmpl_slope_to_se(corner_sw)
	tmpl_slope_to_se(corner_nw)
	tmpl_slope_to_se(small_ne)
	tmpl_slope_to_se(small_se)
	tmpl_slope_to_se(small_sw)
	tmpl_slope_to_se(small_nw)

	tmpl_slope_to_ne(bank_ne)
	tmpl_slope_to_ne(bank_se)
	tmpl_slope_to_ne(bank_sw)
	tmpl_slope_to_ne(bank_nw)
