		[
			STORE_TEMP(                (tile_height > (snowline_height - 2)), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height - 1)), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height    )), 0),
			STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height + 1)), 0),
			LOAD_TEMP(0)
		] ) {
	0: switch_riverbank_grid_height_arctic;
	1: switch_riverbank_grid_arctic_snow14;
	2: switch_riverbank_grid_arctic_snow24;
	3: switch_riverbank_grid_arctic_snow34;
	switch_riverbank_grid_height_snow;
}

switch( FEAT_CANALS, SELF, switch_riverbank_grid_terrain_alpine,
	[
	STORE_TEMP(                (tile_height > (snowline_height - 2)), 0),
	STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height - 1)), 0),
	STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height    )), 0),
	STORE_TEMP( LOAD_TEMP(0) + (tile_height > (snowline_height + 1)), 0),
	LOAD_TEMP(0)
	] ) {
	0: switch_riverbank_grid_height_temperate;
	1: switch_riverbank_grid_alpine_snow14;
	2: switch_riverbank_grid_alpine_snow24;
	3: switch_riverbank_grid_alpine_snow34;
	switch_riverbank_grid_height_snow;
}


switch( FEAT_CANALS, SELF, switch_riverbank_grid_climate, landscape_param) {
	CLIMATE_ARCTIC:   switch_riverbank_grid_terrain_arctic;
	CLIMATE_TROPICAL: switch_riverbank_grid_height_tropical;
	CLIMATE_TOYLAND:  switch_riverbank_grid_height_toyland;
	CLIMATE_ALPINE:   switch_riverbank_grid_terrain_alpine;
	switch_riverbank_grid_height_temperate;
}

